<body class="eateries">
  <section class="eateries">
    <table>
      <tbody>
        <tr>
          <th>
            <h2>Name</h2>
          </th>
          <th>
            <h2>Website</h2>
          </th>
        </tr>
        <% @eateries.each do |eatery| %>
        <tr>
          <td><%= eatery.name %></td>
          <td><a href="<%= eatery.url %>" class="eateries"><%=eatery.name%></td>
            <td><a href="/eateries/<%=eatery.id%>" class="eateries">Show Eatery</a></td><br>
          </td>
        </tr>

        <% end %>
      </tbody>
    </table>
  </section>
</body>
